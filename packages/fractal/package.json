{
  "name": "@snowball-tech/fractal",
  "version": "2.8.5",
  "description": "Fractal's (Snowball's design system) React component library based on RadixUI and PandaCSS",
  "author": "Snowball Engineering Team <engineering@snowball.xyz> (https://github.com/orgs/snowball-tech)",
  "contributors": [
    "Clément PRÉVOT <clement@snowball.xyz> (https://github.com/clementprevot)"
  ],
  "license": "MIT",
  "keywords": [
    "snowball",
    "design-system",
    "fractal",
    "react",
    "panda-css",
    "radix-ui",
    "storybook",
    "chromatic"
  ],
  "homepage": "https://github.com/snowball-tech/fractal/tree/main/packages/fractal",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowball-tech/fractal.git",
    "directory": "packages/fractal"
  },
  "bugs": "https://github.com/snowball-tech/fractal/issues",
  "packageManager": "yarn@3.6.1",
  "engines": {
    "yarn": "^3.0.0",
    "node": "^18.0.0"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build:chromatic": "yarn run -T chromatic --build-script-name build:storybook",
    "prebuild:storybook": "yarn run -T turbo run update:fonts build:code build:styles --filter=@snowball-tech/fractal --parallel",
    "build:storybook": "yarn run prebuild:storybook && yarn storybook build",
    "prebuild": "yarn run clean:build && mkdir -p ./dist",
    "build": "yarn run prebuild && yarn run -T turbo build:code build:info build:styles --filter=@snowball-tech/fractal --parallel --force && yarn run -T turbo build:library --filter=@snowball-tech/fractal && yarn run postbuild",
    "postbuild": "yarn run csso --comments none --input dist/fractal.css --source-map file --output dist/fractal.css --stat && yarn run build:stats",
    "build:stats": "./scripts/build-stats.sh",
    "prebuild:library": "mkdir -p ./dist",
    "build:library": "yarn tsup",
    "build:lib": "yarn run build:library",
    "build:code": "yarn panda codegen",
    "prebuild:info": "mkdir -p ./dist",
    "build:info": "yarn run prebuild:info && yarn panda ship --outfile dist/fractal.buildinfo.json",
    "prebuild:styles": "mkdir -p ./dist",
    "build:styles": "yarn run prebuild:styles && yarn panda cssgen --outfile dist/fractal.css",
    "clean:cache": "rm -Rf ./node_modules/.cache >/dev/null 2>&1 || true",
    "clean:build": "rm -Rf ./dist >/dev/null 2>&1 || true",
    "clean:fonts": "rm -Rf ./assets/fonts >/dev/null 2>&1 || true",
    "clean:storybook": "rm -Rf storybook-static >/dev/null 2>&1 || true",
    "predev": "yarn run -T turbo run update:fonts --filter=@snowball-tech/fractal",
    "dev": "yarn run predev && yarn run -T concurrently \"yarn run -T nodemon -e \\\"\\\" --watch  \\\"./fractal-panda.preset.ts\\\" --exec \\\"yarn run build:code\\\"\" \"yarn run dev:storybook\"",
    "dev:storybook": "yarn storybook dev -p 6006 --no-open",
    "format": "yarn run -T prettier --cache --check .",
    "format:fix": "yarn run -T prettier --cache --write .",
    "lint": "yarn run lint:scripts && yarn run lint:styles",
    "lint:fix": "yarn run lint:scripts:fix && yarn run lint:styles:fix",
    "prelint:scripts": "yarn run -T turbo run build:code --filter=@snowball-tech/fractal",
    "lint:scripts": "yarn run prelint:scripts && yarn run -T eslint --cache .",
    "lint:scripts:fix": "yarn run prelint:scripts && yarn run -T eslint --cache --fix .",
    "lint:styles": "yarn run -T stylelint --cache \"./**/*.{html,css,jsx,tsx}\" \"!storybook-static\"",
    "lint:styles:fix": "yarn run -T stylelint --cache --fix \"./**/*.{html,css,jsx,tsx}\" \"!storybook-static\"",
    "setup": "yarn install && yarn run -T turbo run build:code --filter=@snowball-tech/fractal",
    "start": "yarn run -T serve ./storybook-static",
    "pretypes:check": "yarn run -T turbo run build:code --filter=@snowball-tech/fractal",
    "types:check": "yarn run pretypes:check && tsc --noEmit",
    "preupdate:fonts": "yarn run clean:fonts",
    "update:env": "yarn run -T is-ci || yarn vercel env pull",
    "update:fonts": "yarn run preupdate:fonts && ../../scripts/update-fonts.sh ./assets/"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.9.0",
    "@pandacss/preset-base": "^0.11.0",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@snowball-tech/eslint-config": "^5.1.4",
    "@storybook/addon-a11y": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addons": "^7.2.1",
    "@storybook/blocks": "^7.2.1",
    "@storybook/components": "^7.2.1",
    "@storybook/manager-api": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.2.1",
    "@types/eslint": "^8.44.1",
    "@types/lodash": "^4.14.196",
    "@types/prettier": "^3.0.0",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "csso-cli": "^4.0.2",
    "eslint": "^8.46.0",
    "postcss": "^8.4.27",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^3.0.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.2.1",
    "storybook-addon-mock": "^4.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vercel": "^31.2.2",
    "vite": "^4.4.8",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@ark-ui/react": "^0.10.0",
    "@iconscout/react-unicons": "^2.0.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-compose-refs": "^1.0.1",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@snowball-tech/design-tokens": "5.10.1",
    "country-flag-icons": "^1.5.7",
    "libphonenumber-js": "^1.10.39",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "@iconscout/react-unicons": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
