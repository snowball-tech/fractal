{
  "name": "@snowball-tech/fractal",
  "version": "3.4.1",
  "description": "Fractal's (Snowball's design system) React component library based on RadixUI and PandaCSS",
  "author": "Snowball Engineering Team <engineering@snowball.xyz> (https://github.com/orgs/snowball-tech)",
  "contributors": [
    "Clément PRÉVOT <clement@snowball.xyz> (https://github.com/clementprevot)"
  ],
  "license": "MIT",
  "keywords": [
    "snowball",
    "design-system",
    "fractal",
    "react",
    "panda-css",
    "radix-ui",
    "storybook",
    "chromatic"
  ],
  "homepage": "https://github.com/snowball-tech/fractal/tree/main/packages/fractal",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowball-tech/fractal.git",
    "directory": "packages/fractal"
  },
  "bugs": "https://github.com/snowball-tech/fractal/issues",
  "packageManager": "yarn@4.0.1",
  "engines": {
    "yarn": "^4.0.0",
    "node": "^18.0.0"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build-chromatic": "yarn run -T chromatic --build-script-name build-storybook",
    "prebuild-vercel": "yarn run update-fonts & yarn run build-code & yarn run build-styles",
    "build-vercel": "yarn run prebuild-vercel && yarn storybook build",
    "build-storybook": "yarn run build-vercel",
    "prebuild": "yarn run clean-build && mkdir -p ./dist",
    "build": "yarn run prebuild && (yarn run build-code & yarn run build-info & yarn run build-styles) && yarn build-library && yarn run postbuild",
    "postbuild": "yarn run csso --comments none --input dist/fractal.css --source-map file --output dist/fractal.css --stat && yarn run build-stats",
    "build-stats": "./scripts/build-stats.sh",
    "prebuild-library": "mkdir -p ./dist",
    "build-library": "yarn run prebuild-library && NODE_OPTIONS=--max_old_space_size=16384 yarn tsup",
    "build-lib": "yarn run build-library",
    "build-code": "yarn panda codegen",
    "prebuild-info": "mkdir -p ./dist",
    "build-info": "yarn run prebuild-info && yarn panda ship --outfile dist/fractal.buildinfo.json",
    "prebuild-styles": "mkdir -p ./dist",
    "build-styles": "yarn run prebuild-styles && yarn panda cssgen --outfile dist/fractal.css",
    "clean-cache": "(rm -Rf ./node_modules/.cache >/dev/null 2>&1 || true) & (rm -Rf ../../node_modules/.cache >/dev/null 2>&1 || true)",
    "clean-build": "rm -Rf ./dist >/dev/null 2>&1 || true",
    "clean-fonts": "rm -Rf ./assets/fonts >/dev/null 2>&1 || true",
    "clean-storybook": "rm -Rf storybook-static >/dev/null 2>&1 || true",
    "predev": "yarn run update-fonts",
    "dev": "yarn run predev && yarn run -T nodemon -e \"\" --watch  \"./fractal-panda.preset.ts\" --exec \"yarn run build-code\" & yarn run dev-storybook",
    "dev-storybook": "yarn storybook dev -p 6006 --no-open",
    "format": "yarn run -T prettier --cache --check .",
    "format-fix": "yarn run -T prettier --cache --write .",
    "lint": "yarn run lint-scripts & yarn run lint-styles",
    "lint-fix": "yarn run lint-scripts-fix & yarn run lint-styles-fix",
    "prelint-scripts": "yarn run build-code",
    "lint-scripts": "yarn run prelint-scripts && yarn run -T eslint --cache .",
    "lint-scripts-fix": "yarn run prelint-scripts && yarn run -T eslint --cache --fix .",
    "lint-styles": "yarn run -T stylelint --cache \"./**/*.{html,css,jsx,tsx}\" \"!storybook-static\"",
    "lint-styles-fix": "yarn run -T stylelint --cache --fix \"./**/*.{html,css,jsx,tsx}\" \"!storybook-static\"",
    "presetup": "yarn run -T is-ci || yarn install",
    "setup": "yarn run presetup && yarn run build-code",
    "start": "yarn run -T serve ./storybook-static",
    "pretypes-check": "yarn run build-code",
    "types-check": "yarn run pretypes-check && tsc --noEmit",
    "preupdate-fonts": "yarn run clean-fonts",
    "update-env": "yarn run -T is-ci || yarn vercel env pull",
    "update-fonts": "yarn run preupdate-fonts && ../../scripts/update-fonts.sh ./assets/"
  },
  "devDependencies": {
    "@pandacss/dev": "0.18.0",
    "@pandacss/preset-base": "0.18.0",
    "@snowball-tech/eslint-config": "^5.3.1",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addons": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/components": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.3",
    "@types/eslint": "^8.44.7",
    "@types/lodash": "^4.14.200",
    "@types/prettier": "^3.0.0",
    "@types/prop-types": "^15.7.9",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "chromatic": "^7.6.0",
    "csso-cli": "^4.0.2",
    "eslint": "^8.53.0",
    "postcss": "^8.4.31",
    "postcss-styled-syntax": "^0.5.0",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.5.3",
    "storybook-addon-mock": "^4.3.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vercel": "^32.5.0",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@iconscout/react-unicons": "Azuky/react-unicons",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-compose-refs": "^1.0.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@snowball-tech/design-tokens": "6.1.1",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/preview-api": "^7.5.3",
    "country-flag-icons": "^1.5.7",
    "libphonenumber-js": "^1.10.49",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-textarea-autosize": "^8.5.3"
  },
  "peerDependencies": {
    "@iconscout/react-unicons": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
